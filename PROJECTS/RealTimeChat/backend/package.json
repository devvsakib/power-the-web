{
	"name": "express-boilerplate",
	"version": "1.0.0",
	"description": "Express boilerplate",
	"main": "src/App.js",
	"scripts": {
		"build": "babel ./src --out-dir ./build",
		"clean": "rimraf ./build",
		"clean:logs": "rimraf ./Logs",
		"start": "cross-env NODE_ENV=production node ./build/index.js",
		"dev": "cross-env NODE_ENV=development nodemon --exec npx babel-node src/index.js",
		"lint:check": "eslint src/**/*.{js,json}",
		"lint:fix": "eslint --fix src/**/*.{js,json}",
		"prettier:fix": "prettier -w \"src/**/*.{js,json}\"",
		"prettier:check": "prettier -c \"src/**/*.{js,json}\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:watch:all": "jest --watchAll",
		"test:coverage": "jest --coverage",
		"test:changed": "jest -o"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/kennarddh/express-boilerplate"
	},
	"keywords": [
		"nodejs",
		"boilerplate",
		"babel",
		"express",
		"eslint"
	],
	"author": "kennarddh",
	"license": "ISC",
	"dependencies": {
		"compression": "^1.7.4",
		"cors": "^2.8.5",
		"dotenv": "^16.0.2",
		"express": "^4.17.2",
		"helmet": "^6.0.0",
		"socket.io": "^4.5.2",
		"winston": "^3.8.2",
		"winston-daily-rotate-file": "^4.7.1"
	},
	"devDependencies": {
		"@babel/cli": "^7.18.10",
		"@babel/core": "^7.19.3",
		"@babel/node": "^7.19.1",
		"@babel/preset-env": "^7.19.3",
		"cross-env": "^7.0.3",
		"eslint": "^8.24.0",
		"eslint-config-airbnb-base": "^15.0.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jest": "^27.0.2",
		"eslint-plugin-json": "^3.1.0",
		"eslint-plugin-prettier": "^4.2.1",
		"eslint-plugin-security": "^1.5.0",
		"jest": "^29.1.2",
		"nodemon": "^2.0.20",
		"prettier": "^2.7.1",
		"rimraf": "^3.0.2",
		"supertest": "^6.2.4"
	}
}
